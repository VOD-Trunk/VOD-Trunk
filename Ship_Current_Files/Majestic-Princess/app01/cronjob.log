# Tomcat log cleaning
30 3 * * *  /usr/bin/find /var/log/tomcat7/ -type f -mtime +1 -name "*20[0-9][0-9]-[0-9][0-9]-[0-9][0-9]*" ! -name "*.gz" -exec gzip -vf > /dev/null 2>&1 {} \;
0 3 * * *  /usr/bin/find /var/log/tomcat7/ -type f -mtime +30 -delete
# MySQL binary log rotation
12 2 * * * /usr/bin/mysqladmin flush-logs
13 2 * * * /usr/bin/mysql -e "PURGE BINARY LOGS BEFORE '$(date --date="2 days ago" "+%F %T")'"
#Tmpwatch
5 5 * * * /usr/sbin/tmpwatch -mM --fuser --exclude-pattern=/tmp/hsperfdata* --exclude-pattern=/tmp/*.sock 720 /tmp /var/tmp

