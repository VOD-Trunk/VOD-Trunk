<Host name="prod.uiexm.com" 
appBase="/var/lib/tomcat7/hosts/prod.uiexm.com/webapps"
unpackWARs="true" autoDeploy="false" deployXML="false">
<Alias>app01</Alias>
<Alias>app02</Alias>
<Alias>admin.iptv.kodmdomain.hal.com</Alias>
<Alias>client.iptv.kodmdomain.hal.com</Alias>
<Alias>admin</Alias>
